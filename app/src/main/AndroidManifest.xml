<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chessgame">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".mainpage.matching_mode.chesssetting" />
        <activity android:name=".loginpage.login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainpage.MainActivity" />
        <activity android:name=".loginpage.registeraccount" />
        <activity android:name=".mainpage.matching_mode.vscomputerchessgame" />
        <activity android:name=".mainpage.load_firebase_record.load_record" />



        <activity android:name=".mainpage.feedback_page.user_login_questionaire"></activity>
        <activity android:name=".mainpage.feedback_page.matching_mode_questionaire"></activity>
        <activity android:name=".mainpage.feedback_page.chess_engine_questionaire"></activity>
        <activity android:name=".mainpage.feedback_page.feedback_section"></activity>
        <activity android:name=".mainpage.feedback_page.suggest_function_section"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>